vars
  w1;w2;c1;c2;g1;g2;b1;b2

rules
  w1,g1,!b1 -> !g1;
  w2,g2,!b2 -> !g2;
  g1,c1,!b1 -> !c1;
  g2,c2,!b2 -> !c2;

  b1,g1 -> !g1,!b1,g2,b2;
  b2,g2 -> !g2,!b2,g1,b1;
  b1,c1 -> !c1,!b1,c2,b2;
  b2,c2 -> !c2,!b2,c1,b1;
  b1,w1 -> !w1,!b1,w2,b2;
  b2,w2 -> !w2,!b2,w1,b1;

  b1 -> !b1,b2;
  b2 -> !b2,b1

init
  w1,c1,g1,b1
  
check (3)
  w1,
  !g2,
  !w1,
  <>g2,
  <>(w2 & b2),
  []g2,
  Uz.((c2 & (g2 & w2)) | <>:z)



